{
  "name": "ms-node-tunnel",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "dev-client": "ts-node-dev src/client.ts",
    "dev-server": "ts-node-dev src/tunnel.ts",
    "tunnel": "node dist/server/relayServer.js",
    "client": "node dist/client/localClient.js",
    "dev:tunnel": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server/tunnel.ts",
    "dev:client": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/client/client.ts",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/http-proxy": "^1.17.15",
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "@types/express": "^4.17.21",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.3",
    "ws": "^8.18.0"
  }
}
